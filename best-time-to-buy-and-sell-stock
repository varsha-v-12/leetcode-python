# LeetCode 1: Two Sum
# Approach: Hash Map
# Time Complexity: O(n)
# Space Complexity: O(n)
class Solution(object):
    def maxProfit(self, prices):
        
        if not prices:
            return 0

        min_price = prices[0]
        max_profit = 0

        for i in prices[1:]:
            if i < min_price:
                min_price = i
            else:
                max_profit = max(max_profit, i - min_price)

        return max_profit
